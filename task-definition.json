{
  "family": "api-monedas-task-definition",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::500532068218:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::500532068218:role/api-monedas-task-role",
  "containerDefinitions": [
    {
      "name": "api-monedas-container",
      "image": "052737073980.dkr.ecr.us-east-2.amazonaws.com/monedas-api-ecr:00a3103",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/api-monedas",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  }
}